---
layout: null
permalink: /feed.json
---
{
    "version": "https://jsonfeed.org/version/1",
    "title": {{ challenge.title | smartify | jsonify }},
    "home_page_url": "{{ site.url }}",
    "feed_url": "{{ site.url }}/feed.json",
    "expired": false,
    "_project": [
{% for post in site.challenges%}
        {
            "project_id": "{{ post.challenge-id}}",
            "project_name": "{{ post.challenge-title }}",
            "project_url_on_catalog": "{{ site.url }}/{{ post.permalink }}",
            "project_url_external": "{{ post.external-url }}",
            "card-image": "{{ site.url }}/{{ post.card-image }}",
            "agency-logo": "{{ site.url }}/{{ post.agency-logo }}",
            "prizes": "{{ post.prizes }}",
            "project_description": "{{ post.tagline | strip | escape }}",
            "keywords": "{{ post.legal-authority }}",
            "fields_of_science": "{{ post.type-of-challenge }}",
            "total-prize-offered-cash": "{{ post.total-prize-offered-cash }}",
            "project_status": "{{ post.status}}",
            "agency_sponsor": "{{ post.agency }}",
            "agency_email": "{{ post.challenge-manager-email }}",
            "agency_sponsor_other": "{{ post.partner-agencies-federal }}",
            "challenge-manager": "{{ post.challenge-manager }}",
            "challenge-manager-email": "{{ post.challenge-manager-email }}",
            "start_date": "{{ post.submission-start }}",
            "end_date": "{{ post.submission-end }}",
            "fiscal-year": "{{ post.fiscal-year }}"            
        }{% unless forloop.last == true %},{% endunless %}
{% endfor %}
    ]
}
